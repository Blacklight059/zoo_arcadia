{% extends 'base.html.twig' %}

{% block title %}HabitatComment{% endblock %}

{% block body %}
    <h1>HabitatComment</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ habitat_comment.id }}</td>
            </tr>
            <tr>
                <th>Comment</th>
                <td>{{ habitat_comment.comment }}</td>
            </tr>
            <tr>
                <th>CommentDate</th>
                <td>{{ habitat_comment.commentDate ? habitat_comment.commentDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_habitat_comment_index') }}">back to list</a>

    <a href="{{ path('app_habitat_comment_edit', {'id': habitat_comment.id}) }}">edit</a>

    {{ include('habitat_comment/_delete_form.html.twig') }}
{% endblock %}
