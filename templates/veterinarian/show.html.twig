{% extends 'base.html.twig' %}

{% block title %}Veterinarian{% endblock %}

{% block body %}
    <h3 class="m-3">Vétérinaire</h3>

    <table class="table m-2">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ veterinarian.id }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ veterinarian.email }}</td>
            </tr>
            <tr>
                <th>Roles</th>
                <td>{{ veterinarian.roles ? veterinarian.roles|json_encode : '' }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ veterinarian.password }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ veterinarian.name }}</td>
            </tr>
            <tr>
                <th>Firstname</th>
                <td>{{ veterinarian.firstname }}</td>
            </tr>
        </tbody>
    </table>

    <a class="btn-back m-3" href="{{ path('app_veterinarian_index') }}">Retour à la iste</a>

    <a href="{{ path('app_veterinarian_edit', {'id': veterinarian.id}) }}">edit</a>

    {{ include('veterinarian/_delete_form.html.twig') }}
{% endblock %}
