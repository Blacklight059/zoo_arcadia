{% extends 'base.html.twig' %}

{% block title %}VetReport{% endblock %}

{% block body %}
    <h1>VetReport</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ vet_report.id }}</td>
            </tr>
            <tr>
                <th>AnimalState</th>
                <td>{{ vet_report.animalState }}</td>
            </tr>
            <tr>
                <th>FoodOffered</th>
                <td>{{ vet_report.foodOffered }}</td>
            </tr>
            <tr>
                <th>FoodWeight</th>
                <td>{{ vet_report.foodWeight }}</td>
            </tr>
            <tr>
                <th>VisitDate</th>
                <td>{{ vet_report.visitDate ? vet_report.visitDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>StateDetails</th>
                <td>{{ vet_report.stateDetails }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_vet_report_index') }}">back to list</a>

    <a href="{{ path('app_vet_report_edit', {'id': vet_report.id}) }}">edit</a>

    {{ include('vet_report/_delete_form.html.twig') }}
{% endblock %}
